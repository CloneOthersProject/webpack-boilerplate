    "build": "PRODUCTION=1 npm run app",
    "lint": "eslint --ignore-pattern '/dist/' '**/*.{js,vue}'"
  },
  "dependencies": {
    "vue": "1.0.26",
    "vue-router": "0.7.13",
    "vuex": "0.6.3",
