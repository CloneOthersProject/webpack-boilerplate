{
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmk123/vuex-boilerplate.git"
  },
  "scripts": {
    "_vendor": "node build/build-vendor",
    "dev": "npm run _vendor && webpack --config build/webpack.app.js",
    "start": "npm run _vendor && webpack-dev-server --inline --config build/webpack.devServer.js",
    "build": "PRODUCTION=1 npm run dev",
    "lint": "eslint --ignore-pattern '/dist/' '**/*.{js,vue}'",
    "deploy": "npm run lint && npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "fastclick": "1.0.6",
    "lodash": "4.17.2",
    "normalize.css": "5.0.0",
    "vue": "2.1.3",
    "vue-router": "2.0.3",
    "vuex": "2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "clean-webpack-plugin": "0.1.14",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.0",
    "eslint": "3.11.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-html": "1.7.0",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-standard": "2.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.12.0",
    "html-webpack-plugin": "2.24.1",
    "node-sass": "3.13.0",
    "openpack": "1.0.0",
    "postcss-loader": "1.1.1",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "vue-html-loader": "1.2.3",
    "vue-loader": "10.0.2",
    "vue-template-compiler": "2.1.3",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  },
  "author": "Mingkai Li <milk.lee@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmk123/vuex-boilerplate/issues"
  },
  "homepage": "https://github.com/lmk123/vuex-boilerplate#readme"
}
