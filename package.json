{
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmk123/vuex-boilerplate.git"
  },
  "scripts": {
    "vendor": "node build/build-vendor",
    "app": "npm run vendor && webpack --config build/webpack.app.js",
