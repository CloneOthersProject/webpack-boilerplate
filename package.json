    "normalize.css": "4.2.0",
    "lodash": "4.15.0"
  },
  "devDependencies": {
    "openpack": "1.0.0",
    "babel-core": "6.14.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.14.0",

    "babel-loader": "6.2.5",
    "vue-loader": "8.5.2",
    "vue-html-loader": "1.2.3",
    "file-loader": "0.9.0",
    "postcss-loader": "0.13.0",
    "autoprefixer": "6.4.1",
    "node-sass": "3.9.3",
    "sass-loader": "4.0.2",
    "css-loader": "0.25.0",
